rm *.o rm *.a
gcc -g -c dmdtau.c dmdtau_r.c dora.c fermibubble.c frb_d.c galcen.c gum.c lmc.c localbubble.c ne_crd.c nps.c smc.c spiral.c thick.c thin.c ymw16par.c
ar -rvs ymw16.a *.o   
gcc -g  ymw16.c ymw16.a  -o ymw16 -lm
gcc -g  ymw16_ne.c ymw16.a  -o ymw16_ne -lm
gcc -shared -Wl,-install_name,ymw16lib.so -o ymw16lib.so -lm -fPIC dmdtau.c dmdtau_r.c dora.c fermibubble.c frb_d.c galcen.c gum.c lmc.c localbubble.c ne_crd.c nps.c smc.c spiral.c thick.c thin.c ymw16par.c ymw16.c
rm *.o
